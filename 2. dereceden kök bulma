print("2.Dereceden Bir Denklemin Kökünü Bulma",
"y=ax^2+bx+c")

a = int(input("a : "))
b = int(input("b : "))
c = int(input("c : "))

delta = b ** 2 - 4 * a * c

if delta > 0:
    x1 = (-b + (delta ** (1/2))) / (2 * a)
    x2 = (-b - (delta ** (1/2))) / (2 * a)
    print(x1, x2)
elif delta == 0:
    x = -b / (2*a)
    print(x)
else:
    print("reel kok yok")


